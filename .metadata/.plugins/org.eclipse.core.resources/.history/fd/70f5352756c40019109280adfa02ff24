package javaSorting;

public class Insertion_sort {

	public static void main(String[] args) {
		
		int[] num={10,20,80,50,40};
		int temp;
		for(int i=1;i<num.length;i++){
			
			for(int j=i-1;j>=0;j--){
				if(num[j]>num[i]){
					temp=num[j];
					num[j]=num[i];
					num[i]=temp;
				}
			} i--;
		}
		
		
		for(int n=0;n<num.length;n++){
			System.out.println(num[n]);
		}
	}

}
